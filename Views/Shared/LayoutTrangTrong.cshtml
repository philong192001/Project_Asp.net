    <script>
        $(function () {
            $("#slider-range").slider({
                range: true,
                min: 0,
                max: 100000000,
                values: [0, 100000000],
                slide: function (event, ui) {
                    $("#amount").val(ui.values[0].toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,") + "VNĐ"
                        + "-" + ui.values[1].toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,") + "VNĐ");
                }
            });
            $("#amount").val($("#slider-range").slider("values", 0) + "VNĐ" + "-"
                + $("#slider-range").slider("values", 1) + " VNĐ");
        });
    </script>
