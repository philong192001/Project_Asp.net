@*load template LayoutTrangChu.cshtml vao day*@
@{
    Layout = "~/Views/Shared/LayoutTrangChu.cshtml";
}
@* load thu muc Models de su dung cac class ben trong *@
@using Add_TemplateAdmin.Models
@functions{
    public DataContext db = new DataContext();
    //sam pham noi bat
    public List<Products> HotProduct()
    {
        //.Take(6) -> lay 6 ban ghi
        var _listRecord = db.Products.Where(tbl => tbl.hot == 1).OrderByDescending(tbl => tbl.id).Take(4).ToList();
        return _listRecord != null ? _listRecord : new List<Products>();
    }
    //sam pham moi
    public List<Products> NewProduct()
    {
        //.Take(6) -> lay 6 ban ghi
        var _listRecord = db.Products.OrderByDescending(tbl => tbl.id).Take(4).ToList();
        return _listRecord != null ? _listRecord : new List<Products>();
    }
    //liet ke cac danh muc hien thi trang chu
    public List<Categories> ListCategories()
    {
        var _listRecord = db.Categories.Where(tbl => tbl.displayhome == 1).OrderByDescending(tbl => tbl.id).ToList();
        return _listRecord != null ? _listRecord : new List<Categories>();
    }
    //liet ke cac san pham thuoc danh muc
    public List<Products> ListProducts(int _category_id)
    {
        //.Take(6) -> lay 6 ban ghi
        var _listRecord = db.Products.Where(tbl => tbl.category_id == _category_id).OrderByDescending(tbl => tbl.id).Take(4).ToList();
        return _listRecord != null ? _listRecord : new List<Products>();
    }
}
<style>
    div.stars {
        width: 270px;
        display: inline-block;
    }

    input.star {
        display: none;
    }

    label.star {
        padding: 3px;
        font-size: 30px;
        color: #444;
        transition: all .2s;
    }

        label.star:hover {
            transform: rotate(-15deg) scale(1.3);
            content: '\f005';
            color: #FD4;
            transition: all .25s;
            text-shadow: 0 0 20px #952;
        }

        label.star:before {
            content: '\f006';
            font-family: FontAwesome;
        }
</style>
